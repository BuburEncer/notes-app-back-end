{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "quotes": "off",
    "no-unused-vars": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "spaced-comment": "off",
    "camelcase": "off",
    "object-curly-newline": "off",
    "no-undef": "off"
  }
}
